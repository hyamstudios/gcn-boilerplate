{
  "private": true,
  "name": "gatsby-starter-gcn",
  "description": "A starter template to build amazing static websites with Gatsby, Contentful and Netlify",
  "version": "1.2.3",
  "repository": "https://github.com/ryanwiemer/gatsby-starter-gcn",
  "author": "Ryan Wiemer <ryan@ryanwiemer.com>",
  "dependencies": {
    "babel-plugin-styled-components": "^1.5.1",
    "chalk": "^2.4.1",
    "contentful-import": "^7.2.0",
    "gatsby": "^2.0.12",
    "gatsby-image": "^2.0.12",
    "gatsby-plugin-canonical-urls": "^2.0.5",
    "gatsby-plugin-google-analytics": "^2.0.6",
    "gatsby-plugin-manifest": "^2.0.4",
    "gatsby-plugin-netlify": "^2.0.0",
    "gatsby-plugin-nprogress": "^2.0.5",
    "gatsby-plugin-offline": "^2.0.5",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-sitemap": "^2.0.1",
    "gatsby-plugin-styled-components": "^3.0.0",
    "gatsby-source-contentful": "^2.0.1",
    "inquirer": "^6.2.0",
    "marksy": "^6.1.0",
    "path": "^0.12.7",
    "prismjs": "^1.15.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-helmet": "^5.2.0",
    "react-motion": "^0.5.2",
    "rebass": "^2.3.4",
    "styled-components": "^3.4.5",
    "styled-system": "^3.1.6",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "whatwg-fetch": "^2.0.4"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "dev": "yarn develop",
    "dev-preview": "GATSBY_PREVIEW=true yarn dev",
    "test": "npm run lint:css && npm run lint:js && npm run check-pretty",
    "setup": "node ./bin/setup.js",
    "lint:js": "eslint --ext .js,.jsx .",
    "lint:css": "stylelint 'src/**/*.js'",
    "check-pretty": "prettier -l {,src/**/}*.{js,jsx,json,css,scss}",
    "format": "prettier --write \"src/**/*.js\""
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-gatsby-standard": "^1.2.2",
    "prettier": "^1.14.2",
    "stylelint": "9.5.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.3.2"
  },
  "browserslist": [
    "> 1%",
    "IE >= 11",
    "last 3 versions"
  ]
}
